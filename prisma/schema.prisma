generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model Tasks {
  id        String  @id @default(auto()) @map("_id") @db.ObjectId
  completed Boolean
  name      String
}

model Users {
  id    String @id @default(auto()) @map("_id") @db.ObjectId
  nome  String
  senha String
}
